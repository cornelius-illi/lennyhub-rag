# OpenAI API Key for RAG system
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your-api-key-here

# Optional: Customize working directory
# WORKING_DIR=./rag_storage

# Optional: Enable/disable multimodal processing
# ENABLE_IMAGE_PROCESSING=False
# ENABLE_TABLE_PROCESSING=False
# ENABLE_EQUATION_PROCESSING=False

# Qdrant Vector Database Configuration
# Set to 'true' to use Qdrant, 'false' to use default NanoVectorDB
USE_QDRANT=true

# URL for Qdrant server (local instance)
QDRANT_URL=http://localhost:6333

# Note: Collection names are auto-generated by LightRAG as:
# lightrag_vdb_{namespace}_{embedding_model}_{dimensions}d
# Example: lightrag_vdb_chunks_text_embedding_3_small_1536d
